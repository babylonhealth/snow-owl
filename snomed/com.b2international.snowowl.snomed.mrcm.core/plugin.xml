            terminologyId="com.b2international.snowowl.terminology.snomed">
      </serviceConfigJob>
   </extension>
   <extension
         point="com.b2international.snowowl.core.terminologyComponent">
      <terminologyComponent
            decorator="icons/mrcm_editor.gif"
            hierarchical="false"
            icon="icons/mrcm_editor.gif"
